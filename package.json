		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"tailwindcss": "4",
		"terser": "^5.43.1",
		"typescript": "~5.6.2",
		"vite": "^6.0.3",
		"vitest": "^3.2.4"
	}
}
