{
  "name": "irisnotes-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n main,quick -c blue,magenta \"pnpm main\" \"pnpm quick\"",
    "main": "pnpm -C apps/main tauri dev --config src-tauri/tauri.dev.conf.json",
    "quick": "pnpm -C apps/quick tauri dev --config src-tauri/tauri.dev.conf.json",
    "build": "pnpm build:main && pnpm build:quick",
    "build:main": "pnpm -C apps/main tauri build",
    "build:quick": "pnpm -C apps/quick tauri build",
    "cli": "cargo run --manifest-path apps/main/src-tauri/Cargo.toml --",
    "type-check": "pnpm -C apps/main run type-check",
    "test": "pnpm -C apps/main run test",
    "test:e2e": "pnpm -C apps/main run test:e2e"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
